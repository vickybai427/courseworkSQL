7.1
CREATE OR REPLACE PACKAGE ORDER_INFO_PKG IS FUNCTION
SHIP_NAME_PF (p_basket IN NUMBER)
RETURN VARCHAR2;
PROCEDURE BASKET_INFOR_PP
(P_BASKET IN NUMBER,
P_SHOP OUT NUMBER,
P_DATE OUT DATE);
END;
/

